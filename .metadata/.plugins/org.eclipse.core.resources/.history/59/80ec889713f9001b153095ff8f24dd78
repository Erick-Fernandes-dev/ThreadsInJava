package be.com.alura.ExercicioThread02;

public class RespostaMain {
	
	public static void main(String[] args) {
		
		Thread thread = new Thread(new Runnable() {
			
			@Override
			public void run() {
				
				System.out.println("Hello world");
				
			}
		});
		
		thread.start();
		
		
		
		
	}

}
